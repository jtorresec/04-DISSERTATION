money <- out.money %>%
transform(Data = as.Date(Data)) %>%
mutate(Ano = year(Data)) %>%
mutate(Mes = month(Data))%>%
select(Ano, Valor) %>%
filter(Ano != 1994) %>%
group_by(Ano) %>%
summarise(M0 = max(Valor)/1000000000000) #%>%
ggplot(money, aes(x = Ano)) +
geom_line(aes(y = M0))+
labs(y = ' Trilhões R$ correntes', x = NULL)
